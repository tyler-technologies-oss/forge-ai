{
  "name": "@ai-foundry/sdk",
  "version": "0.1.0",
  "description": "SDK for integrating Tyler AI Foundry capabilities into applications.",
  "type": "module",
  "main": "./dist/sdk.js",
  "types": "./dist/sdk.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sdk.d.ts",
      "default": "./dist/sdk.js"
    },
    "./ui/*": {
      "types": "./dist/ui/*/.d.ts",
      "default": "./dist/ui/*/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:npm && pnpm run build:cdn",
    "build:npm": "tsc && vite build",
    "build:cdn": "vite build --config vite.config.cdn.ts",
    "preview": "vite preview",
    "sri": "node scripts/sri-generator.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "ai",
    "chatbot",
    "embed",
    "sdk",
    "tyler",
    "forge"
  ],
  "author": "Tyler Technologies",
  "license": "Apache-2.0",
  "dependencies": {
    "lit": "catalog:"
  },
  "peerDependencies": {
    "@tylertech/forge-ai": "workspace:"
  },
  "devDependencies": {
    "@repo/prettier-config": "workspace:",
    "@tylertech/forge": "catalog:",
    "@tylertech/tyler-icons": "catalog:",
    "@types/node": "^24.10.1",
    "eslint": "catalog:",
    "typescript": "~5.9.3",
    "vite": "catalog:",
    "vite-plugin-dts": "catalog:"
  }
}
