{
  "name": "@ai-foundry/sdk",
  "version": "0.1.0",
  "description": "SDK for integrating Tyler AI Foundry capabilities into applications.",
  "type": "module",
  "main": "./dist/sdk.js",
  "types": "./dist/sdk.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sdk.d.ts",
      "default": "./dist/sdk.js"
    },
    "./ui/*": {
      "types": "./dist/ui/*/index.d.ts",
      "default": "./dist/ui/*/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "default": "./dist/react/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "customElements": "custom-elements.json",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:npm && pnpm run build:cdn && pnpm run cem",
    "build:npm": "tsc && vite build",
    "build:cdn": "vite build --config vite.config.cdn.ts",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "cem": "cem analyze --config custom-elements-manifest.config.js --litelement && pnpm run generate-proxies",
    "generate-proxies": "node scripts/generate-proxies.mjs",
    "inspect-bundle": "vite-bundle-visualizer --config vite.config.cdn.ts"
  },
  "keywords": [
    "ai",
    "chatbot",
    "embed",
    "sdk",
    "tyler",
    "forge"
  ],
  "author": "Tyler Technologies",
  "license": "Apache-2.0",
  "dependencies": {
    "@tylertech/forge-ai": "workspace:",
    "lit": "catalog:"
  },
  "peerDependencies": {
    "@tylertech/forge": "^3.0.0"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "catalog:",
    "@repo/prettier-config": "workspace:",
    "@tylertech/forge": "catalog:",
    "@tylertech/tyler-icons": "catalog:",
    "@types/node": "^24.10.4",
    "change-case": "^5.4.4",
    "custom-element-react-wrappers": "^1.7.3",
    "eslint": "catalog:",
    "glob": "catalog:",
    "typescript": "~5.9.3",
    "vite": "catalog:",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-dts": "catalog:"
  }
}
